{
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8,
        "modules": true,
        "ecmaFeatures": {
            "impliedStrict": true,
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "shared-node-browser": true,
        "es6": true,
        "jasmine": false
    },
    "globals": {
        "module": false,
        "require": false,
        "rb": false,
        "appGlobals": true,
        "lazySizes": false,
        "__webpack_public_path__": true,
        "process": false,
        "System": false
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:import/errors"],
    "rules": {
        "block-scoped-var": [2],
        "default-case": [0],
        "no-caller": [2],
        "no-console": [1],
        "no-unsafe-negation": [2],
        "no-eval": [2],
        "no-implicit-globals": [2],
        "no-implied-eval": [2],
        "no-loop-func": [2],
        "no-script-url": [2],
        "no-self-compare": [2],
        "comma-dangle": [1, "always-multiline"],
        "object-curly-spacing": [1, "always"],
        "no-trailing-spaces": 1,
        "curly": [2, "multi-line"],
        "no-unused-vars": [2, { "argsIgnorePattern": "^_", "args": "after-used", "varsIgnorePattern": "_"}],
        "quotes": [2, "single", { "avoidEscape": true, "allowTemplateLiterals": true }],
        "radix": [2],
        "no-with": [1],
        "no-useless-escape": [2],
        "no-useless-concat": [2],
        "no-useless-call": [2],
        "semi": [2],
        "react/jsx-no-bind": [2],
        "import/first": [2],
        "import/no-unresolved": [0]
    }
}
